{
  "$Version": "4.0",
  "$Reference": {
    "https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.UI.V1.json": {
      "$Include": [{ "$Namespace": "Org.OData.UI.V1", "$Alias": "UI" }]
    },
    "https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Common.V1.json": {
      "$Include": [{ "$Namespace": "Org.OData.Common.V1", "$Alias": "Common" }]
    }
  },
  "UI.examples": {
    "$Alias": "self",
    "NoteType": {
      "$Kind": "EntityType",
      "$Key": ["NoteBasicUUID", "IsActiveEntity"],
      "NoteBasicUUID": { "$Type": "Edm.Guid" },
      "NoteBasicObjectNodeType": { "$MaxLength": 30 },
      "NoteBasicObject": { "$MaxLength": 90 },
      "NoteBasicType": { "$MaxLength": 20 },
      "NoteBasicTypeDescription": { "$MaxLength": 255 },
      "NoteBasicLanguage": { "$MaxLength": 2 },
      "LanguageName": { "$MaxLength": 16 },
      "NoteBasicTitle": { "$MaxLength": 255 },
      "NoteBasicMIMEType": { "$MaxLength": 127 },
      "NoteBasicLength": { "$Type": "Edm.Int32" },
      "CreatedByUser": { "$MaxLength": 12 },
      "CreationDateTime": { "$Type": "Edm.DateTimeOffset", "$Nullable": true, "$Precision": 7 },
      "LastChangedByUser": { "$MaxLength": 12 },
      "LastChangedByUserDescription": { "$MaxLength": 80 },
      "LastChangeDateTime": { "$Type": "Edm.DateTimeOffset", "$Nullable": true, "$Precision": 7 },
      "NoteBasicContent": {},
      "HasDraftEntity": { "$Type": "Edm.Boolean" },
      "DraftEntityCreationDateTime": { "$Type": "Edm.DateTimeOffset", "$Nullable": true, "$Precision": 7 },
      "DraftEntityLastChangeDateTime": { "$Type": "Edm.DateTimeOffset", "$Nullable": true, "$Precision": 7 },
      "HasActiveEntity": { "$Type": "Edm.Boolean" },
      "IsActiveEntity": { "$Type": "Edm.Boolean" }
    },
    "$Annotations": {
      "self.NoteType": {
        "@UI.Note": {
          "Title": { "$Path": "NoteBasicTitle" },
          "ContentType": { "$Path": "NoteBasicMIMEType" },
          "Content": { "$Path": "NoteBasicContent" },
          "Type": { "$Path": "NoteBasicType" },
          "TypeDescription": { "$Path": "NoteBasicTypeDescription" },
          "Language": { "$Path": "NoteBasicLanguage" },
          "LanguageName": { "$Path": "NoteBasicLanguageName" },
          "ObjectNodeType": { "$Path": "NoteBasicType/NoteBasicObjectNodeType" },
          "LanguageDependent": { "$Path": "NoteBasicType/NoteBasicLanguageDependent" },
          "MaximalLength": { "$Path": "NoteBasicType/NoteBasicMaximalLength" },
          "MultipleNotes": { "$Path": "NoteBasicType/NoteBasicMultipleNotes" },
          "HideTitle": { "$Path": "NoteBasicType/NoteBasicHideTitle" },
          "IsTextPlain": { "$Path": "NoteBasicType/NoteBasicIsTextPlain" }
        },
        "@Common.CreatedBy": { "$Path": "CreatedByUser" },
        "@Common.CreatedAt": { "$Path": "CreationDateTime" },
        "@Common.ChangedBy": { "$Path": "LastChangedByUser" },
        "@Common.ChangedAt": { "$Path": "LastChangeDateTime" }
      }
    }
  }
}
