{
    "$Version": "4.0",
    "$Reference": {
        "https://sap.github.io/odata-vocabularies/vocabularies/Analytics.json": {
            "$Include": [
                {
                    "$Namespace": "com.sap.vocabularies.Analytics.v1",
                    "$Alias": "Analytics"
                }
            ]
        },
        "https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Aggregation.V1.json": {
            "$Include": [
                {
                    "$Namespace": "Org.OData.Aggregation.V1",
                    "$Alias": "Aggregation"
                }
            ]
        },
        "https://oasis-tcs.github.io/odata-vocabularies/examples/Org.OData.Aggregation.V1.SalesModel-sample.xml": {
            "$Include": [
                {
                    "$Namespace": "org.example.odata.salesservice",
                    "$Alias": "SalesModel"
                }
            ]
        }
    },
    "example.Analytics.UpdateAction": {
        "$Alias": "this",
        "UpdateAggregateSales": [
            {
                "$Kind": "Action",
                "@Core.Description": "Updates an aggregate entity of type `SalesModel.Sale`",
                "$Parameter": [
                    {
                        "$Name": "Apply",
                        "@Core.Description": "String identifying the aggregate entity to be updated"
                    },
                    {
                        "$Name": "Changes",
                        "$Type": "SalesModel.Sale",
                        "@Core.Description": "Aggregate entity with nested properties that are to be changed, like the body of a PATCH request"
                    }
                ],
                "$ReturnType": {
                    "$Type": "SalesModel.Sale",
                    "@Core.Description": "Aggregate entity with changes applied"
                }
            }
        ],
        "container": {
            "$Kind": "EntityContainer",
            "UpdateAggregateSales": {
                "$Action": "this.UpdateAggregateSales",
                "$EntitySet": "SalesModel.SalesData/Sales"
            }
        },
        "$Annotations": {
            "SalesModel.SalesData/Sales": {
                "@Aggregation.CustomAggregate#Amount": "Edm.Decimal",
                "@Analytics.CustomDisaggregate#Amount": {
                    "@odata.type": "https://sap.github.io/odata-vocabularies/vocabularies/Analytics.xml#Analytics.ProportionalDisaggregateType",
                    "InProportionTo": {
                        "$Path": "Amount"
                    }
                }
            }
        }
    },
    "$EntityContainer": "example.Analytics.UpdateAction.container"
}