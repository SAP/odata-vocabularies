{
    "$Version": "4.0",
    "$Reference": {
        "https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Aggregation.V1.json": {
            "$Include": [
                {
                    "$Namespace": "Org.OData.Aggregation.V1",
                    "$Alias": "Aggregation"
                }
            ]
        },
        "https://sap.github.io/odata-vocabularies/vocabularies/UI.json": {
            "$Include": [
                {
                    "$Namespace": "com.sap.vocabularies.UI.v1",
                    "$Alias": "UI"
                }
            ]
        },
        "https://sap.github.io/odata-vocabularies/vocabularies/Analytics.json": {
            "$Include": [
                {
                    "$Namespace": "com.sap.vocabularies.Analytics.v1",
                    "$Alias": "Analytics"
                }
            ]
        }
    },
    "DynamicProperties.examples": {
        "Sales": {
            "$Kind": "EntityType",
            "$Key": [
                "Order"
            ],
            "Order": {},
            "Amount": {
                "$Type": "Edm.Decimal",
                "$Nullable": true,
                "$Scale": 0
            },
            "@Analytics.AggregatedProperties": [
                {
                    "Name": "Total",
                    "AggregationMethod": "sum",
                    "AggregatableProperty": "Amount"
                },
                {
                    "Name": "Maximum",
                    "AggregationMethod": "max",
                    "AggregatableProperty": "Amount"
                }
            ],
            "@UI.Chart": {
                "ChartType": "Bubble",
                "Dimensions": [
                    "Country"
                ],
                "Measures": [
                    "NumberOfOrders"
                ],
                "DynamicMeasures": [
                    "@Analytics.AggregatedProperties/0",
                    "/Container/me/SalesOrders@Aggregation.CustomAggregate#WeightedAverage"
                ]
            },
            "@UI.PresentationVariant": {
                "SortOrder": [
                    {
                        "Property": "Country"
                    },
                    {
                        "DynamicProperty": "@Analytics.AggregatedProperties/0"
                    },
                    {
                        "DynamicProperty": "/Container/me/SalesOrders@Aggregation.CustomAggregate#WeightedAverage"
                    }
                ]
            }
        },
        "meType": {
            "$Kind": "EntityType",
            "SalesOrders": {
                "$Kind": "NavigationProperty",
                "$Collection": true,
                "$Type": "DynamicProperties.examples.Sales",
                "$ContainsTarget": true
            }
        },
        "Container": {
            "$Kind": "EntityContainer",
            "me": {
                "$Type": "DynamicProperties.examples.meType"
            }
        },
        "$Annotations": {
            "Container/me/SalesOrders": {
                "@Aggregation.CustomAggregate#WeightedAverage": "Edm.Decimal"
            }
        }
    },
    "$EntityContainer": "DynamicProperties.examples.Container"
}